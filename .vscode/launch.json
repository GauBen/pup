{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno: Debug basic example",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "-A",
        "--unstable",
        "--inspect",
        "pup.ts",
        "run",
        "--cwd",
        "docs/examples/basic"
      ],
      "port": 9229
    },
    {
      "name": "Deno: Run Example: Basic",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "-A",
        "--unstable",
        "pup.ts",
        "run",
        "--cwd",
        "docs/examples/basic"
      ],
      "attachSimplePort": 9229
    }
  ]
}
