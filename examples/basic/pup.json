{
  "processes": [
    {
      "name": "periodic-example-task",
      "cmd": ["deno", "run", "--allow-read", "./task.js"],
      "startPattern": "*/5 * * * * *"
    },
    {
      "name": "kept-alive-server",
      "cmd": ["deno", "run", "--allow-read", "./server.js"],
      "autostart": true,
      "restart": "always",
      "restartDelayMs": 10000
    }
  ]
}
